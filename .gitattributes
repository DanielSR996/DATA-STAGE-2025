# Auto detect text files and perform LF normalization
* text=auto

# JSX files
*.jsx text eol=lf

# JavaScript files
*.js text eol=lf

# JSON files
*.json text eol=lf

# CSS files
*.css text eol=lf

# Markdown files
*.md text eol=lf

# Package files
package.json text eol=lf
package-lock.json text eol=lf
yarn.lock text eol=lf

# Binary files
*.png binary
*.jpg binary
*.gif binary
*.ico binary
*.ttf binary
*.woff binary
*.woff2 binary 